{
  "name": "apiv3",
  "version": "1.0.0",
  "repository": "https://github.com/joog-lim/Bamboo-APIv3.git",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "start": "sls offline start",
    "deploy": "sls deploy",
    "test-code": "tsc && node dist/test.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config ./ormconfig.json",
    "migration": "yarn migration:remove && yarn migration:create &&  yarn migration:generate && yarn typeorm migration:run",
    "migration:create": "yarn typeorm migration:create -n auto_script",
    "migration:generate": "yarn typeorm migration:generate -n auto_script",
    "migration:remove": "rm -rf src/migration || del /s /Q src\\migration"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^2.7.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.84",
    "@types/node": "^16.11.7",
    "serverless": "^2.64.1",
    "serverless-dotenv-plugin": "^3.10.0",
    "serverless-offline": "^8.2.0",
    "serverless-plugin-typescript": "^2.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
